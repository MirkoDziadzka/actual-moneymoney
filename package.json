    "license": "MIT",
    "dependencies": {
        "@actual-app/api": "file:actual-api.tgz",
        "app-root-path": "^3.1.0",
        "date-fns": "^2.30.0",
        "dotenv": "^16.0.3",
        "env-paths": "^3.0.0",
        "lowdb": "^6.0.1",
        "plist": "^3.0.6",
        "prompts": "^2.4.2",
        "run-applescript": "^6.1.0",
        "yargs": "^17.7.2"
    },
    "node": ">=16",
    "devDependencies": {
        "@types/plist": "^3.0.2",
        "@types/prompts": "^2.4.4",
        "@types/yargs": "^17.0.24",
        "eslint": "^8.39.0",
        "nodemon": "^2.0.22",
        "prettier": "^2.8.8",
        "ts-node": "^10.9.1",
        "typescript": "^5.0.4"
    }
